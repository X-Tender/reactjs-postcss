{
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 7,
		"ecmaFeatures": {
			"jsx": false,
			"spread": true,
			"classes": true,
			"experimentalObjectRestSpread": true
		}
	},
	"parser": "babel-eslint",
	"globals": {
		"require": false,
		"__dirname": false,
		"module": false,
		"process": true,
		"ga": true
	},
	"plugins": ["babel", "react", "react-hooks", "prettier"],
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["react-app", "prettier", "prettier/react"],
	"settings": {
		"react": {
			"createClass": "createReactClass",
			"pragma": "React",
			"version": "16.0"
		},
		"propWrapperFunctions": ["forbidExtraProps"]
	},
	"rules": {
		"no-console": 0,
		"no-extra-parens": 0,
		"no-process-env": 0,

		"no-control-regex": 1,
		"no-unreachable": 1,
		"valid-jsdoc": 1,
		"no-empty": 1,
		"no-extra-semi": 1,
		"no-inner-declarations": [1, "functions"],
		"no-extra-boolean-cast": 1,
		"no-debugger": 1,
		"comma-dangle": ["warn", "always-multiline"],

		"no-cond-assign": [2, "always"],
		"no-constant-condition": 2,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty-character-class": 2,
		"no-ex-assign": 2,
		"no-func-assign": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-unsafe-negation": 2,
		"no-obj-calls": 2,
		"no-regex-spaces": 2,
		"no-sparse-arrays": 2,
		"no-unexpected-multiline": 2,
		"use-isnan": 2,
		"valid-typeof": 2,

		"curly": 0,
		"default-case": 0,
		"dot-location": 0,
		"guard-for-in": 0,
		"no-alert": 0,
		"no-eq-null": 0,
		"no-invalid-this": 0,
		"no-iterator": 0,
		"no-labels": 0,
		"no-magic-numbers": 0,
		"no-new-func": 0,
		"no-param-reassign": 0,
		"no-new-wrappers": 0,
		"no-script-url": 0,
		"vars-on-top": 0,
		"radix": 0,
		"complexity": [0, 4],
		"no-new": 0,
		"no-multi-spaces": [
			0,
			{
				"exceptions": {
					"VariableDeclarator": true
				}
			}
		],

		"dot-notation": [
			1,
			{
				"allowKeywords": true,
				"allowPattern": ""
			}
		],
		"no-div-regex": 1,
		"no-else-return": 1,
		"no-extend-native": 1,
		"no-fallthrough": 1,
		"no-floating-decimal": 1,
		"no-lone-blocks": 1,
		"no-octal": 1,
		"no-octal-escape": 1,
		"no-useless-call": 1,
		"no-warning-comments": [
			1,
			{
				"terms": ["todo", "fixme"],
				"location": "anywhere"
			}
		],
		"no-useless-concat": 1,
		"no-shadow": [
			1,
			{
				"hoist": "functions",
				"allow": ["e", "event", "i", "j", "k", "$", "jQuery", "event"]
			}
		],

		"accessor-pairs": 2,
		"block-scoped-var": 2,
		"consistent-return": 2,
		"eqeqeq": [2, "allow-null"],
		"no-caller": 2,
		"no-case-declarations": 2,
		"no-empty-pattern": 2,
		"no-eval": 2,
		"no-extra-bind": 2,
		"no-implicit-coercion": [
			2,
			{
				"boolean": true,
				"number": false,
				"string": true
			}
		],
		"no-implied-eval": 2,
		"no-loop-func": 2,
		"no-multi-str": 2,
		"no-global-assign": 2,
		"no-unused-expressions": 0,
		"babel/quotes": [1, "single", "avoid-escape"],
		"no-throw-literal": 2,
		"no-sequences": 2,
		"no-self-compare": 2,
		"no-return-assign": 2,
		"no-redeclare": [
			2,
			{
				"builtinGlobals": true
			}
		],
		"no-proto": 2,
		"yoda": 2,
		"wrap-iife": [2, "any"],
		"no-with": 2,
		"no-void": 2,
		"no-shadow-restricted-names": 2,
		"jsx-quotes": 2,

		"strict": 0,

		"no-undefined": 0,
		"init-declarations": 0,

		"no-unused-vars": [
			1,
			{
				"vars": "local",
				"args": "after-used",
				"argsIgnorePattern": "(^e$|^event$|^response$|^data$|^res$)"
			}
		],
		"no-undef-init": 1,

		"no-use-before-define": [2, "nofunc"],
		"no-label-var": 2,
		"no-delete-var": 2,
		"no-undef": [
			2,
			{
				"typeof": false
			}
		],

		"callback-return": 0,
		"global-require": 0,
		"handle-callback-err": 0,
		"no-mixed-requires": 0,
		"no-new-require": 0,
		"no-path-concat": 0,
		"no-process-exit": 0,
		"no-restricted-modules": 0,
		"no-sync": 0,

		"array-bracket-spacing": 0,
		"block-spacing": 0,
		"brace-style": 0,
		"camelcase": 0,
		"consistent-this": 0,
		"computed-property-spacing": 0,
		"comma-spacing": [
			0,
			{
				"before": false,
				"after": true
			}
		],
		"eol-last": 0,
		"indent": 0,
		"id-match": 0,
		"id-length": 0,
		"func-style": 0,
		"max-statements": 0,
		"max-params": 0,
		"max-len": 0,
		"lines-around-comment": 0,
		"key-spacing": 0,
		"no-underscore-dangle": 0,
		"no-trailing-spaces": 0,
		"no-ternary": 0,
		"func-call-spacing": 1,
		"no-restricted-syntax": 0,
		"no-plusplus": 0,
		"no-new-object": 0,
		"no-array-constructor": 0,
		"no-negated-condition": 0,
		"no-multiple-empty-lines": 0,
		"no-mixed-spaces-and-tabs": 0,
		"no-inline-comments": 0,
		"no-continue": 0,
		"no-bitwise": 0,
		"padding-line-between-statements": 0,
		"sort-vars": 0,
		"require-jsdoc": 0,
		"padded-blocks": 0,
		"one-var": [1, { "initialized": "never", "uninitialized": "always" }],
		"object-curly-spacing": 0,
		"wrap-regex": 0,
		"spaced-comment": 0,
		"space-unary-ops": [
			0,
			{
				"words": true,
				"nonwords": false
			}
		],
		"keyword-spacing": 1,
		"space-infix-ops": 0,
		"space-in-parens": 0,
		"space-before-function-paren": 0,
		"space-before-blocks": 0,
		"func-names": 0,
		"max-nested-callbacks": [0, 4],
		"semi-spacing": [
			0,
			{
				"before": false,
				"after": true
			}
		],
		"max-depth": [0, 5],

		"comma-style": [2, "last"],
		"linebreak-style": 1,
		"no-useless-constructor": 1,
		"new-cap": 1,
		"no-unneeded-ternary": 1,
		"no-nested-ternary": 1,
		"new-parens": 1,
		"semi": [1, "always"],
		"quotes": [1, "single", "avoid-escape"],
		"operator-linebreak": [
			1,
			"after",
			{
				"overrides": {
					"?": "before",
					":": "before"
				}
			}
		],
		"operator-assignment": [1, "always"],
		"quote-props": [1, "as-needed"],
		"no-lonely-if": 1,

		"prefer-template": 1,
		"prefer-spread": 1,
		"prefer-arrow-callback": 1,
		"object-shorthand": 1,
		"arrow-spacing": [1, { "before": true, "after": true }],
		"arrow-parens": [1, "as-needed"],
		"arrow-body-style": [1, "as-needed"],
		"prefer-const": 1,

		"require-yield": 2,
		"no-var": 2,
		"no-this-before-super": 2,
		"no-dupe-class-members": 2,
		"no-const-assign": 2,
		"no-class-assign": 2,
		"generator-star-spacing": [2, { "before": false, "after": true }],
		"constructor-super": 2,

		"no-await-in-loop": 2,
		"no-misleading-character-class": 1,
		"require-unicode-regexp": 0,
		"prefer-object-spread": 1,
		"max-classes-per-file": 0,
		"max-lines-per-function": 0,

		"babel/no-unused-expressions": [
			2,
			{
				"allowTernary": true,
				"allowShortCircuit": true,
				"allowTaggedTemplates": true
			}
		],

		"react/boolean-prop-naming": ["error", { "rule": "^(is|has)[A-Z]([A-Za-z0-9]?)+" }],
		"react/button-has-type": 2,
		"react/destructuring-assignment": 0,
		"react/display-name": 0,
		"react/forbid-component-props": 0,
		"react/forbid-dom-props": 0,
		"react/forbid-elements": 0,
		"react/forbid-prop-types": 0,
		"react/forbid-foreign-prop-types": 0,
		"react/no-access-state-in-setstate": 2,
		"react/no-array-index-key": 1,
		"react/no-children-prop": 1,
		"react/no-danger": 1,
		"react/no-danger-with-children": 1,
		"react/no-deprecated": 2,
		"react/no-did-mount-set-state": 2,
		"react/no-did-update-set-state": 2,
		"react/no-direct-mutation-state": 2,
		"react/no-find-dom-node": 1,
		"react/no-is-mounted": 2,
		"react/no-multi-comp": [1, { "ignoreStateless": true }],
		"react/no-redundant-should-component-update": 1,
		"react/no-render-return-value": 2,
		"react/no-set-state": 0,
		"react/no-typos": 2,
		"react/no-string-refs": 2,
		"react/no-this-in-sfc": 2,
		"react/no-unescaped-entities": 1,
		"react/no-unknown-property": 1,
		"react/no-unused-state": 1,
		"react/no-will-update-set-state": 2,
		"react/prefer-es6-class": 2,
		"react/prefer-stateless-function": 0,
		"react/prop-types": 2,
		"react/react-in-jsx-scope": 2,
		"react/require-default-props": 2,
		"react/require-optimization": 0,
		"react/require-render-return": 2,
		"react/self-closing-comp": 1,
		"react/sort-comp": [
			1,
			{
				"order": ["type-annotations", "static-methods", "lifecycle", "everything-else", "render"]
			}
		],
		"react/sort-prop-types": 0,
		"react/style-prop-object": 2,
		"react/void-dom-elements-no-children": 2,

		"react/jsx-boolean-value": 1,
		"react/jsx-child-element-spacing": 1,
		"react/jsx-closing-bracket-location": [1, "tag-aligned"],
		"react/jsx-curly-spacing": 1,
		"react/jsx-equals-spacing": 1,
		"react/jsx-filename-extension": 2,
		"react/jsx-first-prop-new-line": [1, "multiline-multiprop"],
		"react/jsx-handler-names": 0,
		"react/jsx-indent": 0,
		"react/jsx-indent-props": 0,
		"react/jsx-key": 2,
		"react/jsx-max-depth": 0,
		"react/jsx-max-props-per-line": 0,
		"react/jsx-no-bind": 1,
		"react/jsx-no-comment-textnodes": 2,
		"react/jsx-no-duplicate-props": 2,
		"react/jsx-no-literals": 0,
		"react/jsx-no-target-blank": 1,
		"react/jsx-no-undef": 2,
		"react/jsx-one-expression-per-line": 0,
		"react/jsx-curly-brace-presence": [2, { "props": "never", "children": "never" }],
		"react/jsx-pascal-case": 2,
		"react/jsx-sort-props": 1,
		"react/jsx-space-before-closing": 0,
		"react/jsx-tag-spacing": 0,
		"react/jsx-uses-react": 2,
		"react/jsx-uses-vars": 1,
		"react/jsx-wrap-multilines": 1,
		"react-hooks/rules-of-hooks": 2,
		"react-hooks/exhaustive-deps": 1
	}
}
