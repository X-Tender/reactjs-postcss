{
	"name": "reactjs-postcss",
	"version": "1.0.0",
	"description": "A ReactJS Boilerplate made beautiful with postCSS",
	"main": "cfg/index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "npm-run-all --parallel build:js build:css",
		"build:js": "webpack --config cfg/webpack.config.prod.js",
		"build:debug": "webpack --config cfg/webpack.config.debug.js",
		"build:css": "NODE_ENV=production postcss --map false 'src/scss/*.css' --dir 'public/assets/css' --config cfg/postcss.config.js",
		"style": "postcss 'src/scss/*.css' --watch --dir 'public/assets/css' --config cfg/postcss.config.js",
		"code": "node .",
		"code:hidden": "node . --h",
		"server": "http-server -s",
		"start": "npm-run-all --parallel server style code",
		"start:hidden": "npm-run-all --parallel style code:hidden"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/X-Tender/reactjs-postcss"
	},
	"keywords": ["react", "reactjs", "php", "postcss", "boilerplate", "slim"],
	"author": "Paul Kamma",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/X-Tender/reactjs-postcss/issues"
	},
	"homepage": "https://github.com/X-Tender/reactjs-postcss#readme",
